{
	"extends": ["@commitlint/config-conventional"],
	"rules": {
		"type-enum": [
			2,
			"always",
			[
				"perf",
				"demo",
				"merge",
				"feat",
				"fix",
				"docs",
				"style",
				"refactor",
				"test",
				"revert",
				"build",
				"ci",
				"chore",
				"conflict",
				"delete",
				"release"
			]
		]
	}
}
