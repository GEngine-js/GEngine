{
  "name": "GEngine",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged --pattern \"**/*.*(js|md|html)\"",
    "pre-build": "node hooks/checkDependency.js",
    "install:npm": "npm install --registry=https://registry.npmmirror.com",
		"install:yarn": "yarn install --registry=https://registry.npmmirror.com",
    "build": "npm run pre-build && cross-env NODE_ENV=production rollup -c",
    "dev": "npm run pre-build && cross-env NODE_ENV=development rollup -c"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@types/gl-matrix": "^2.4.5",
    "@webgpu/glslang": "0.0.15",
    "@webgpu/types": "0.1.20",
    "cross-env": "^7.0.3",
    "glob": "^8.0.3",
    "mersenne-twister": "^1.1.0",
    "primitive-geometry": "^2.9.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup": "3.10.0"
  },
  "repository": {
